{
  "name": "dot-ai-ui",
  "version": "0.11.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "tsx watch server/index.ts",
    "build": "tsc -b && vite build",
    "start": "NODE_ENV=production node --import tsx server/index.ts",
    "lint": "eslint .",
    "generate:api": "openapi-generator-cli generate -i http://localhost:8080/api/v1/openapi -g typescript-fetch -o src/api/generated --additional-properties=typescriptThreePlus=true,supportsES6=true",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vfarcic/dot-ai-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vfarcic/dot-ai-ui/issues"
  },
  "homepage": "https://github.com/vfarcic/dot-ai-ui#readme",
  "dependencies": {
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jszip": "^3.10.1",
    "mermaid": "^11.12.2",
    "prismjs": "^1.30.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "yaml": "^2.8.2"
  },
  "overrides": {
    "lodash": "4.17.23",
    "lodash-es": "4.17.23"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@openapitools/openapi-generator-cli": "^2.27.0",
    "@playwright/test": "^1.58.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0"
  }
}
